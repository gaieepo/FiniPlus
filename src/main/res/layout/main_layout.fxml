<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.MainController">
   <center>
      <ListView fx:id="lv" prefHeight="200.0" prefWidth="200.0" />
   </center>
   <left>
      <Label fx:id="toast" text="Label" BorderPane.alignment="CENTER" />
   </left>
   <bottom>
      <Button fx:id="elohim" mnemonicParsing="false" text="Button" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
